# Quinn Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to git!

# Google OAuth Credentials (from Google Cloud Console)
GOOGLE_OAUTH_CLIENT_ID=your-client-id-here.apps.googleusercontent.com
GOOGLE_OAUTH_CLIENT_SECRET=your-client-secret-here

# API Key for bot-to-bot communication
# Generate with: python -c "import secrets; print(secrets.token_urlsafe(32))"
BOT_API_KEY=your-secret-api-key-here

# Optional: Flask secret key (will auto-generate if not set)
# FLASK_SECRET_KEY=your-random-secret-key-here

# Admin Emails (comma-separated list)
# Only these email addresses can manage external staff access
ADMIN_EMAILS=admin1@example.com,admin2@example.com
