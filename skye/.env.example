# Flask secret key for session signing
# Generate with: python -c "import secrets; print(secrets.token_hex(32))"
FLASK_SECRET_KEY=your-secret-key-here
FLASK_DEBUG=False

# Google OAuth credentials
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Bot API key for bot-to-bot communication
BOT_API_KEY=your-shared-bot-api-key

# Environment (development or production)
SKYE_ENV=development

# Admin emails (comma-separated, optional)
# SKYE_ADMIN_EMAILS=admin@watsonblinds.com.au
